<h1>Editing playlist_item</h1>

<% form_for(@playlist_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :item_id %><br />
    <%= f.text_field :item_id %>
  </p>
  <p>
    <%= f.label :item_type_id %><br />
    <%= f.text_field :item_type_id %>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.text_field :position %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @playlist_item %> |
<%= link_to 'Back', playlist_items_path %>